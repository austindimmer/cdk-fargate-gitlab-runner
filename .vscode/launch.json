    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Python: Debug CDK",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "env": {
                "PYTHONPATH": "/workspaces/cdk-fargate-gitlab-runner;/workspaces/cdk-fargate-gitlab-runner/gitlab_ci_fargate_runner;/workspaces/cdk-fargate-gitlab-runner/task_definitions;",
                "LOG_LEVEL": "Debug",
            },
            "console": "integratedTerminal"
        },
    ]
}